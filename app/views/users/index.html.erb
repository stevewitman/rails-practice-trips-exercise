<div class="page-header">
  <h1>Users</h1>
</div>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th> Name </th>
      <th> Email </th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= user.email %>
      </td>
      <% end %>
    </tr>
  </tbody>
</table>
